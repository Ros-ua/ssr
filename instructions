curl -O https://raw.githubusercontent.com/Ros-ua/ssr/master/install_yum.sh && chmod +x install_yum.sh && ./install_yum.sh
apt-get update && apt-get install sudo curl -y && curl -O https://raw.githubusercontent.com/Ros-ua/ssr/master/install_apt.sh && chmod +x install_apt.sh && ./install_apt.sh

wget -N --no-check-certificate https://raw.githubusercontent.com/V2RaySSR/vps/master/vpstest.sh && bash vpstest.sh
